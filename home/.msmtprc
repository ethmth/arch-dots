# ~/.msmtprc

# Define the default account
account default

# SMTP server to use
host mail.protonmail.ch

# SMTP port (usually 25 for unauthenticated connections)
port 25

# No authentication required
auth off

# Optional: define a from address (change as needed)
from pc@ethanmt.com

# Optional: set TLS/SSL (depends on server support)
tls off
